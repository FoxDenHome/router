config interface 'VPN'
	option proto 'wireguard'
	option private_key 'REMOVED'
	option listen_port '13231'
	list addresses '10.100.0.1/16'
	option defaultroute '0'
	option peerdns '0'
	option delegate '0'
	option nohostroute '1'

config interface 'LTE'
	option proto 'modemmanager'
	option device '/sys/devices/platform/soc/3110000.usb3/xhci-hcd.1.auto/usb4/4-1/4-1.2'
	option apn 'Wholesale'
	option auth 'none'
	option peerdns '0'
	option metric '100'
	option signalrate '60'
	option delegate '0'
	option iptype 'ipv4v6'
	option ip6weight '1'

