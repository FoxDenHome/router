
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd49:d005:574c::/48'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth1'
	list ports 'eth2'
	list ports 'eth3'
	list ports 'eth4'
	option mtu '9000'

config interface 'WAN'
	option proto 'dhcp'
	option peerdns '0'
	option metric '50'
	option device 'eth0'

config device
	option name 'eth4'
	option mtu '9000'

config bridge-vlan
	option device 'br-lan'
	option vlan '1'
	list ports 'eth1:u*'
	list ports 'eth4:u*'

config bridge-vlan
	option device 'br-lan'
	option vlan '2'
	list ports 'eth1:t'
	list ports 'eth2:u*'
	list ports 'eth3:u*'
	list ports 'eth4:t'

